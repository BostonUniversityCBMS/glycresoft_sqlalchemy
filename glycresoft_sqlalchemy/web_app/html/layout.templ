<!DOCTYPE html5>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title></title>
    <link rel="stylesheet" href="{{ url_for('static',filename='css/style.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ url_for('static',filename='css/materialize.css') }}">    
    <link rel="stylesheet" type="text/css" href="{{ url_for('static',filename='font/icons.css') }}">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    
    <script src="{{ url_for('static',filename='js/dist/vendor-bundle.js') }}"></script>
    <script src="{{ url_for('static',filename='js/dist/lib-bundle.js') }}"></script>
    <script src="{{ url_for('static',filename='js/dist/app-bundle.js') }}"></script>
    <script>

$(function() {
    $('select').material_select();
    $(".side-nav").css('top', $('.nav-wrapper').innerHeight())
    $(".add-sample").click(function(event){
        event.preventDefault()
        setupAjaxForm("/add_sample", "#dialog-modal")
    })
});



    </script>
</head>
<body>
    {% include "header.templ" %}
    <div class="side-nav fixed">
    <div class='task-list-container'>
        <h6>Tasks</h6>
        <div class='divider'>
        </div>
        <ul>
        </ul>
    </div>
    </div>
    <div class='action-layer-container'>
        <div id='home-layer' class='container'>
        {% block body %}
        {% endblock %}
        </div>        
    </div>
    <div id="dialog-modal" class="modal wide">
        <div class="modal-content">
        </div>
        <div class="modal-footer">
            <a href="#!" class=" modal-action modal-close waves-effect waves-green btn-flat">Dismiss</a>
        </div>
    </div>
    
    {% include "footer.templ" %}
</body>
</html>
